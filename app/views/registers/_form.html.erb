<%= simple_form_for @register  do |form| %>
    <%= form.input :name %>
    <%= form.input :first_name %>
    <%= form.input :email %>
    <%= form.input :phone %>
    <%= form.input :degree %>
    <%= form.input :age %>
    <%= form.input :have_computer %>
    <%= form.input :use_internet %>
    <%= form.input :sex %>
    <%= form.input :twitter_url %>
    <%= form.input :facebook_url %>
    <%= form.input :comment %>

    <%= form.button :submit %>
<% end %>
